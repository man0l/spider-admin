
{% block body %}
<div class="row">
        <div class="col-xs-6 col-md-6">
            <a href="#" class="thumbnail">
              {% if item.image == '' %}
                <img src="data:image/svg+xml;base64," alt="...">
              {% else %}
                <img src="{{ item.image }}">
              {% endif %}
            </a>
        </div>
    </div>
            <div class="row">
                <h3>{{ item.title }}</h3>
                
            </div>
    <div class="row">
        <div class="col-xs-6 col-md-6">
            {{ item.description | nl2br }}
        </div>
    </div>
 {% endblock %}