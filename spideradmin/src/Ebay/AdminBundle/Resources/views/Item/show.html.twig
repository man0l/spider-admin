{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Item</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ebayid</th>
                <td>{{ entity.ebayId }}</td>
            </tr>
            <tr>
                <th>Upc</th>
                <td>{{ entity.upc }}</td>
            </tr>
            <tr>
                <th>Mpn</th>
                <td>{{ entity.mpn }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ entity.brand }}</td>
            </tr>
            <tr>
                <th>Ean</th>
                <td>{{ entity.ean }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ entity.price }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ entity.category }}</td>
            </tr>
            <tr>
                <th>Asin</th>
                <td>{{ entity.asin }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_item') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
